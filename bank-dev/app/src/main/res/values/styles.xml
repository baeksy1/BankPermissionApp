<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="btn_clickable">
        <item name="android:layout_height">52dp</item>
        <item name="android:strokeColor" >#00000000</item>
        <item name="android:elevation" >0dp</item>
        <item name="android:gravity">center</item>
        <item name="fontFamily">@font/scdream4</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:letterSpacing" >0.01</item>
        <item name="android:lineSpacingExtra">5.8sp</item>
        <item name="android:enabled">false</item>
        <item name="android:background">@drawable/btn_bg_clickable_selector</item>
    </style>
